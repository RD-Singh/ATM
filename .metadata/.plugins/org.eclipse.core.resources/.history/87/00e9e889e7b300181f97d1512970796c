


public class Test
{

	public static void main(String[] args)
	{
		
		
		int limit = 100, num1 = 15, num2 = 40;
		if (limit <= limit)
		{
		   if (num1 == num2)
		   {   
		      System.out.println ("lemon");
		      System.out.println ("lime");
		   
		  }
		   else	  
			   System.out.println ("grape");
		}


		
	}
}			